{
  "name": "es6-express-mongoose-jest-boilerplate",
  "version": "1.0.0",
  "description": "A Boilerplate application for building REST APIs using express, mongoose in ES6 and Jest.",
  "author": "Mateusz Skibicki",
  "main": "index.js",
  "private": false,
  "engines": {
    "node": "10.x",
    "npm": ">=6.4.1",
    "yarn": ">=1.5.1"
  },
  "scripts": {
    "start": "npm run serve",
    "dev": "nodemon --exec babel-node server/index.js",
    "build": "babel server --out-dir dist",
    "serve": "NODE_ENV=production node dist/index.js",
    "start:debug": "cross-env DEBUG=es6-express-mongoose-jest-boilerplate:* npm run start",
    "test": "jest --verbose",
    "test-watch": "jest --watchAll --verbose"
  },
  "keywords": [
    "express",
    "node",
    "node.js",
    "mongodb",
    "mongoose",
    "es6",
    "REST",
    "API",
    "boilerplate"
  ],
  "dependencies": {
    "@babel/runtime": "^7.4.3",
    "@types/jest": "^24.0.11",
    "babel-plugin-add-module-exports": "^1.0.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "1.18.2",
    "compression": "1.7.2",
    "cookie-parser": "1.4.3",
    "cors": "2.8.4",
    "debug": "^2.4.5",
    "dotenv": "^4.0.0",
    "express": "4.16.3",
    "express-jwt": "5.3.1",
    "express-winston": "2.5.0",
    "helmet": "3.12.0",
    "http-status": "1.0.1",
    "jsonwebtoken": "7.1.9",
    "lodash": "^4.17.11",
    "method-override": "^2.3.10",
    "mongoose": "4.7.4",
    "morgan": "1.9.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "winston": "2.4.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "cross-env": "5.1.4",
    "eslint": "3.16.1",
    "eslint-config-airbnb-base": "7.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "1.16.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-watch": "2.1.14",
    "husky": "0.14.3",
    "jest": "^24.7.1",
    "nodemon": "^1.18.11",
    "prettier": "^1.17.0",
    "superagent": "^5.0.2",
    "supertest": "^4.0.2",
    "supertest-as-promised": "4.0.2",
    "validate-commit-msg": "^2.14.0"
  },
  "license": "MIT"
}
